{
  "rules": {
    ".read": false,
    ".write": false,
    "users": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        ".write": "auth != null && auth.uid === $uid"
      }
    },
    "analytics": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        "clicks": {
          "$linkId": {
            ".write": true
          }
        }
      }
    }
  }
}
